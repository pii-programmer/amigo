<div class="create-room">
  <%= link_to '別のルームチャットをつくる', new_room_path %>
</div>


<div class="rooms">
  <% current_user.rooms.each do |room| %>
    <div class="room">
      <div class="room-name">
        <%= link_to room.title, room_messages_path(room) %>
      </div>
    </div>
  <% end %>
</div>